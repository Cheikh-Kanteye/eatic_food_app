{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "types": [ 
      "@types/styled-components-react-native"
    ],
    "baseUrl": ".",
    "paths": {
      "@src/*": ["src/*"], 
      "@components/*": ["src/components/*"], 
      "@theme/*": ["src/theme/*"], 
      "@navigation/*": ["src/navigation/*"], 
      "@assets/*": ["src/assets/*"], 
      "@screens/*": ["src/screens/*"], 
      "@utils/*": ["src/utils/*"], 
    }, 
  },  
}
